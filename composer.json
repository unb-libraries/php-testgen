{
    "name": "unb-libraries/trupal",
    "description": "Auto-generate automated testing.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name":"UNB Libraries",
            "email": "libsupport@unb.ca"
        },
        {
            "name": "Patrick Schilf",
            "email": "pschilf@unb.ca"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "symfony/config": "^4.4",
        "symfony/dependency-injection": "^4.4",
        "symfony/yaml": "^4.4",
        "twig/twig": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Trupal\\": "src",
            "Trupal\\Test\\": "tests/phpunit/src"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    }
}
